{
	"pages": [ //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
		{
			// 播放器首页
			"path": "pages/index/index",
			"style": {
				"navigationBarTitleText": "uni-app",
				"app-plus":{
					"titleNView":{
						"backgroundColor":"#ffffff",
						"buttons":[
							{
								"fontSrc":"./static/iconfont.ttf",
								"text":"&#xe63a;",
								"fontSize":"30upx"
							},
							{
								"float":"left",
								"fontSrc":"./static/iconfont.ttf",
								"text":"&#xe64b;",
								"fontSize":"30upx"
							}
						],
						"searchInput":{
							"borderRadius":"30px",
							"align":"left",
							"disabled":true,
							"backgroundColor":"#f7f7f7"
						}
					}
				}
			}
		},
		{
			// 搜索页面
			"path":"pages/search/search",
			"style":{
				"app-plus":{
					"titleNView":{
						"searchInput":{
							"align":"left",
							"backgroundColor":"#f7f7f7",
							"borderRadius":"30upx",
							"placeholder":"See You Again(速度与激情7)",
							"autoFocus":true
						}
					}
				}
			}
		},
		// 详情页面
		{
			"path":"pages/detail/detail",
			"style":{
				"navigationBarBackgroundColor":"rgb(119,14,18)",
				"titleNView":{
					"titleColor":"#c3c3c3"
				}
			}
		},
		{
			"path":"pages/account/account",
			"style":{
				"app-plus":{
					"titleNView":{
						"titleText":"我的账户"
					}
				}
			}
		},
		{
			"path":"pages/friend/friend",
			"style":{
				"app-plus":{
					"titleNView":{
						"titleText":"朋友"
					}
				}
			}
		},
		{
			"path":"pages/mine/mine",
			"style":{
				"app-plus":{
					"titleNView":{
						"titleText":"我的音乐"
					}
				}
			}
		},
		{
			"path":"pages/video/video",
			"style":{
				"app-plus":{
					"titleNView":{
						"titleText":""
					}
				}
			}
		},
		{
			"path":"pages/history/history",
			"style":{
				"app-plus":{
					"titleNView":{
						"titleText":"历史记录"
					}
				}
			}
		}
	],
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "uni-app",
		"navigationBarBackgroundColor": "#FFFFFF",
		"backgroundColor": "#F8F8F8"
	},
	"tabBar":{
		"color":"#969696",
		"selectedColor":"#ff3f33",
		"list":[
			{
				"pagePath":"pages/index/index",
				"text":"发现",
				"iconPath":"./static/icon/index.png",
				"selectedIconPath":"./static/icon/selected-index.png"
			},
			{
				"pagePath":"pages/video/video",
				"text":"视频",
				"iconPath":"./static/icon/video.png",
				"selectedIconPath":"./static/icon/selected-video.png"
			},
			{
				"pagePath":"pages/mine/mine",
				"text":"我的",
				"iconPath":"./static/icon/mine.png",
				"selectedIconPath":"./static/icon/selected-mine.png"
			},
			{
				"pagePath":"pages/friend/friend",
				"text":"朋友",
				"iconPath":"./static/icon/friend.png",
				"selectedIconPath":"./static/icon/selected-friend.png"
			},
			{
				"pagePath":"pages/account/account",
				"text":"账户",
				"iconPath":"./static/icon/account.png",
				"selectedIconPath":"./static/icon/selected-account.png"
			}
		]
		
		
	}
}
