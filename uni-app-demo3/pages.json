{
	"pages": [ //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
		{
			"path": "pages/index/index",
			"style": {
				"navigationBarTitleText": "uni-app",
				"titleNView":{
					"buttons":[
						{
							"type":"share",
							"text":"分享"
						}
					],
					"searchInput":{
						"align":"left",
						"borderRadius":"5px",
						"placeholder":"百度一下，你就知道",
						"placeholderColor":"hotpink"
					}
				}
			}
		},
		{
			"path":"pages/index/login",
			"style":{
				"app-plus":{
					"titleNView":{
						"titleText":"登陆",
						"backgroundColor":"#000",
						"titleColor":"#fff"
					}
				}
			}
		},
		{
			"path":"pages/index/list",
			"style":{
				"app-plus":{
					"titleNView":{
						"titleText":"列表",
						"backgroundColor":"#000",
						"titleColor":"#fff",
						"buttons":[
							{
								"type":"share",
								"color":"red"
							}
						]
					}
				}
			}
		},
		{
			"path":"pages/index/device",
			"style":{
				"navigationBarTitleText":"设备"
			}
		},
		{
			"path":"pages/index/music",
			"style":{
				"app-plus":{
					"titleNView":{
						"titleText":"音乐"
					}
				}
			}
		}
	],
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "uni-app",
		"navigationBarBackgroundColor": "#F8F8F8",
		"backgroundColor": "#F8F8F8"
	},
	"tabBar":{
		"list":[
			{	
				"text":"首页",
				"pagePath":"pages/index/index"
			},
			{
				"text":"列表",
				"pagePath":"pages/index/list"
			},
			{
				"text":"设备",
				"pagePath":"pages/index/device"
			},
			{
				"text":"音乐",
				"pagePath":"pages/index/music"
			}
		]
	}
}
